{
  "name": "@my/theme",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@angular/common": "^10",
    "@angular/core": "^10",
    "@angular/platform-browser": "^10",
