{
  "name": "jest-screenshot-example",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "lerna bootstrap && lerna run build",
    "ng": "ng",
    "start": "lerna run start --scope my-app",
    "build": "lerna run build",
    "e2e": "ENABLE_SCREENSHOTS=1 ng e2e",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "test:watch": "ng test --watch",
    "clean": "rm -rf dist && lerna run clean && lerna clean"
  },
  "private": true,
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.1",
    "@types/jest": "^26.0.13",
    "@types/jest-axe": "^3.5.0",
    "@types/lodash": "^4.14.161",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.6.4",
    "jest": "^26.4.2",
    "jest-preset-angular": "^8.3.1",
    "lerna": "^3.22.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "ng-packagr": "^10.1.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "typescript": "~3.9.5"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
