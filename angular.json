{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "root": {
      "projectType": "library",
      "root": "",
      "sourceRoot": "",
      "prefix": "my",
      "architect": {
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "no-cache": true
          }
        },
        "e2e": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "no-cache": true,
            "configPath": "jest.config.screenshots.js",
            "tsConfig": "tsconfig.screenshots.json"
          }
        }
      }
    }
  },
  "defaultProject": "root"
}
