/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "types": ["jest", "node"],
    "allowJs": true,
    "noImplicitAny": false
  },
  "exclude": [
    "**/*.knobs.ts",
    "__coverage__",
    "__screenshots__"
  ],
  "include": [
    "**/*.spec.ts",
    "**.*.d.ts"
  ]
}
